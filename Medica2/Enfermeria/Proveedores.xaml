        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Medica2.Enfermeria"
        xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation" x:Class="Medica2.Enfermeria.Proveedores"
        mc:Ignorable="d"
        Title="Proveedores" Height="464.4" Width="479.2">
    <Grid>

        <telerik:Label x:Name="lblnombre" Content="Nombre" Margin="21,56,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" />
        <telerik:Label x:Name="lblapaterno" Content="Apellido paterno" HorizontalAlignment="Left" Margin="21,83,0,0" VerticalAlignment="Top"/>
        <telerik:Label x:Name="lblamaterno" Content="Apellido Materno" HorizontalAlignment="Left" Margin="21,111,0,0" VerticalAlignment="Top"/>
        <telerik:Label x:Name="lbledad" Content="Edad" HorizontalAlignment="Left" Margin="21,138,0,0" VerticalAlignment="Top"/>
        <telerik:Label x:Name="lblsexo" Content="Sexo" HorizontalAlignment="Left" Margin="21,165,0,0" VerticalAlignment="Top"/>
        <telerik:Label x:Name="lblcalle" Content="Calle" HorizontalAlignment="Left" Margin="21,193,0,0" VerticalAlignment="Top"/>
        <telerik:Label x:Name="lblciudad" Content="Comunidad" HorizontalAlignment="Left" Margin="21,221,0,0" VerticalAlignment="Top"/>
        <telerik:Label x:Name="lblmunicipio" Content="Municipio" HorizontalAlignment="Left" Margin="21,248,0,0" VerticalAlignment="Top"/>
        <telerik:Label x:Name="lblestado" Content="Estado" HorizontalAlignment="Left" Margin="21,275,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtNombre" HorizontalAlignment="Left" Height="23" Margin="147,57,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txtAmaterno" HorizontalAlignment="Left" Height="23" Margin="147,112,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
