        <TextBox x:Name="txtEstado" HorizontalAlignment="Left" Height="23" Margin="147,276,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txtApaterno" HorizontalAlignment="Left" Height="23" Margin="147,84,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <ComboBox x:Name="cobCalle" HorizontalAlignment="Left" VerticalAlignment="Top" Width="120" Margin="147,194,0,0"/>
        <ComboBox x:Name="cobComunidad" HorizontalAlignment="Left" VerticalAlignment="Top" Width="120" Margin="147,222,0,0"/>
        <ComboBox x:Name="cobmunicipio" HorizontalAlignment="Left" VerticalAlignment="Top" Width="120" Margin="147,249,0,0"/>
        <ComboBox x:Name="cobedad" HorizontalAlignment="Left" VerticalAlignment="Top" Width="120" Margin="147,139,0,0"/>
        <Button x:Name="btnGuargar" Content="Guardar" HorizontalAlignment="Left" Margin="123,333,0,0" VerticalAlignment="Top" Width="75" Click="btnGuargar_Click"/>
        <Button x:Name="btnCancelar" Content="Cancelar" HorizontalAlignment="Left" Margin="275,333,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
