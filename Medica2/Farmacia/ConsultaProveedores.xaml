<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Medica2.Farmacia"
        xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation" x:Class="Medica2.Farmacia.ConsultaProveedores"
        mc:Ignorable="d"
        Title="ConsultaProveedores" Height="412.683" Width="477.073">
    <Grid>

        <telerik:RadGridView Name="radProveedores" HorizontalAlignment="Left" Margin="-1,10,0,0" VerticalAlignment="Top" Height="260" AutoGenerateColumns="False" Width="471">
            <telerik:RadGridView.Columns>
                <telerik:GridViewDataColumn Header="ID" DataMemberBinding="{Binding ID_PROVEEDOR}"/>
                <telerik:GridViewDataColumn Header="RFC" DataMemberBinding="{Binding RFC}"/>
                <telerik:GridViewDataColumn Header="Referencia" DataMemberBinding="{Binding REFERENCIA}"/>
                <telerik:GridViewDataColumn Header="Fecha de Registro" DataMemberBinding="{Binding FECHA_CREACION}"/>
            </telerik:RadGridView.Columns>
        </telerik:RadGridView>

    </Grid>
</Window>
